{
  "name": "@gram-net/wasm",
  "version": "1.2.9",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "author": "William Oney <flcoder@gmail.com>",
  "license": "LGPL-2.1-only",
  "scripts": {
    "dev": "nodemon -w src --exec 'yarn build && yarn test'",
    "watch": "nodemon -w src -w wasm --exec 'yarn build'",
    "build": "yarn lint && tsc --project tsconfig.json && cp -r wasm dist",
    "lint": "eslint src --fix",
    "deploy": "yarn link",
    "test": "nodemon -w dist --exec 'node -r dotenv/config --experimental-modules --enable-source-maps --trace-warnings --experimental-specifier-resolution=node dist/test-node'"
  },
  "engines": {
    "node": "12"
  },
  "devDependencies": {
    "@types/mocha": "~7.0.2",
    "@types/node": "~14.0.5",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "~3.0.0",
    "@typescript-eslint/parser": "~3.0.0",
    "chalk-cli": "~4.1.0",
    "cypress": "~4.7.0",
    "eslint": "~7.1.0",
    "eslint-config-cypress": "~0.28.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-import": "~2.20.2",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~3.1.3",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-standard": "~4.0.1",
    "husky": "~4.2.5",
    "lint-staged": "~10.2.6",
    "mocha": "~7.2.0",
    "nodemon": "~2.0.4",
    "prettier": "~2.0.5",
    "serve": "~11.3.1",
    "typescript": "~3.9.3",
    "yarn": "~1.22.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "form-data": "^3.0.0",
    "tweetnacl": "~1.0.3",
    "uuid": "^7.0.2"
  }
